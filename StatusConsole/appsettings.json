  // This section defines translation of OBC Event Data streams. Byte[0] of a frame is the module id. byte[1] bit 6...0 is the event Id. Bit7/6 in byte[1] defines the 
  // Event severity: 00 info, 01 warning, 10 error 11 fatal. rest of frame can be defined for each event type with names and parameter types as follows:
  "ClimbObcConfig": {
    "MODULES": [
      {
        "Id": 0,
        "Name": "ClimbApplication",
        "Short": "app",
        "Events": [
          {
            "Id": 1,
            "Name": "Full Time Data",
            "Short": "tim",
            "Pars": [
              {
                "Name": "date",
                "Short": "",
                "Type": "uint32",
                "Format": "0000-00-00"
              },
              {
                "Name": "time",
                "Short": "",
                "Type": "uint32",
                "Format": "00:00:00"
              },
              {
                "Name": "tleDay",   
                "Short": "td",
                "Type": "double",
                "Format": "0.00000000"
              },
              {
                "Name": "offset RTC",
                "Short": "o",
                "Type": "double",
                "Format": "0.000s"
              },
              {
                "Name": "offset GPS",
                "Short": "og",
                "Type": "double",
                "Format": "0.000s"
              }
            ]
          },
          {
            "Id": 2,
            "Name": "Raw Data",
            "Short": "raw"
            // If no Pars are defined, the rest of the frame is printed as hex dump. So this is good for this event!
          },
          {
            "Id": 3,
            "Name": "String Event",
            "Short": "str",
            "Pars": [
              {
                "Name": "Text",
                "Type": "char[]" // Holds all Ascii chars until \0 or end of frame.
              }
            ]
          },
          {
            "Id": 4,
            "Name": "System Init",
            "Short": "init"
          },
          {
            "Id": 5,
            "Name": "System Status",
            "Short": "st",
            "Pars": [
              {
                "Name": "LPC1769 Chip Serial number",
                "Short": "LPC",
                "Type": "byte[],4" // This prints 4 bytes as Hex dump 
              },
              {
                "Name": "InstanceName",
                "Short": "Obc",
                "Type": "char[],16" // This prints as string until \0 or end of array. Next Par starts at the byte after.
              },
              {
                "Name": "SDCardName",
                "Short": "Sdc",
                "Type": "char[],20" // This prints as string until \0 or end of array. Next Par starts at the byte after.
              },
              // Allignment !?
              // in struct here starts obc_tim_systemtime_t
              {
                "Name": "EpochNumber",
                "Short": "epoch",
                "Type": "uint32"
              },
              {
                "Name": "MsInEpoch",
                "Short": "ms",
                "Type": "uint32"
              },
              // obc_tim_tledatetime_t  holding synchronized UTC time of last reset ( = epoch starttime)
              {
                "Name": "UtcYear",
                "Short": "y",
                "Type": "uint64"
              },
              {
                "Name": "UtcEpochStart",
                "Short": "doy", // "Day of year" as double coded according to TLE specification.
                "Type": "double"
              },
              // Memory Status
              {
                "Name": "Memory Channel Status",
                "Short": "mems",
                "Type": "byte[],4"
              },
              {
                "Name": "Sd Card Block0",
                "Short": "sdb0",
                "Type": "uint32"
              },
              {
                "Name": "Sd Card Size",
                "Short": "siz",
                "Type": "uint32"
              },
              {
                "Name": "Sd Card Used",
                "Short": "usd",
                "Type": "uint32"
              },
              {
                "Name": "Command Counter",
                "Short": "cmd",
                "Type": "uint32"
              },
              {
                "Name": "Error Counter",
                "Short": "err",
                "Type": "uint32"
              },
              {
                "Name": "SoftwareRelease",
                "Short": "Ver",
                "Type": "char[],16" // This prints as string until \0 or end of array. Next Par starts at the byte after.
              }
            ]
          },
          {
            "Id": 6,
            "Name": "InitReport",
            "Short": "init",
            "Pars": [
              {
                "Name": "PowerOnReset",
                "Short": "PowR",
                "Type": "bit:1"
              },
              {
                "Name": "ExternalReset",
                "Short": "ExtR",
                "Type": "bit:1"
              },
              {
                "Name": "WatchdogReset",
                "Short": "WdR",
                "Type": "bit:1"
              },
              {
                "Name": "BrownOutReset",
                "Short": "BoR",
                "Type": "bit:1"
              },
              {
                "Name": "SysReset",
                "Short": "SyR",
                "Type": "bit:1"
              },
              {
                "Name": "LockOut",
                "Short": "lock",
                "Type": "bit:1"
              },
              {
                "Name": "HardwareWatchdog",
                "Short": "hwWD",
                "Type": "bit:1"
              },
              {
                "Name": "OddEvenReset",
                "Short": "Odd:Even",
                "Type": "bit:1"
              },
              {
                "Name": "RtcPowerUp",
                "Short": "rtcP",
                "Type": "bit:1"
              },
              {
                "Name": "RtcError",
                "Short": "rtcE",
                "Type": "bit:1"
              },
              {
                "Name": "RtcSetDefaults",
                "Short": "rDef",
                "Type": "bit:1"
              },
              {
                "Name": "RtcGprCrcError",
                "Short": "gprE",
                "Type": "bit:1"
              },
              {
