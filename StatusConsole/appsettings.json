{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose"
      }
      },
      "WriteTo:System": {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "MinimumLevel": "Verbose",
            //"Filter": [
            //  {
            //    "Name": "ByExcluding",
            //    "Args": {
            //      "expression": "SourceContext = 'DEBUG-Y1'"
            //    }
            //  }
            //],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "log\\log-2-.log",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] [{SourceContext}] [{EventId}] {Message}{NewLine}{Exception}",
                  "rollOnFileSizeLimit": true,
                  "fileSizeLimitBytes": 4194304,
                  "retainedFileCountLimit": 14,
                  "rollingInterval": "Day"
                }
              }
            ]
          }
        }
      },
      "WriteTo:Serial-Y1": {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "MinimumLevel": "Debug",
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "SourceContext = 'WBusHelper'"
                }
              }
            ],
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "log\\Serial-.log",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] [{SourceContext}] [{EventId}] {Message}{NewLine}{Exception}",
                  "rollOnFileSizeLimit": true,
                  "fileSizeLimitBytes": 4194304,
                  "retainedFileCountLimit": 14,
                  "rollingInterval": "Day"
                }
              }
            ]
          }
        }
      }

    },
  "Logging": {
    "ConGuiConsole": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Error"
      }
    }

  },

